{{#each decorations}}
    {{#is frame.decorationId _id}}
        <div class="fg" style="background-image: url(/uploads/{{fg}});"></div>
        <div class="plan" style="background-image: url(/uploads/{{plan}})"></div>
        <div class="bg" style="background-image: url(/uploads/{{bg}})"></div>
    {{/is}}
{{/each}}
