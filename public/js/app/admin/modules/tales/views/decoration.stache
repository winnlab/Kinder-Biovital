{{#each decorations}}
    {{#is frame.decorationId _id}}
        <div class="img fg" style="background-image: url(/uploads/{{fg}});"></div>
        <div class="img plan" style="background-image: url(/uploads/{{plan}})"></div>
        <div class="img bg" style="background-image: url(/uploads/{{bg}})"></div>
    {{/is}}
{{/each}}
